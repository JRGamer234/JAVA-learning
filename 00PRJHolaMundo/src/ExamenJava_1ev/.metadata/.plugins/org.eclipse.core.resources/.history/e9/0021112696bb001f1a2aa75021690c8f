package src;

import java.util.Scanner;

public class Ejercicio3 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int alt = 10;
		
		for(int i = 0; i < alt ;i++)
		{
			for (int j = 1; j < alt ; j++) {
				System.out.print("*!*!");
			}
			
			System.out.println();
		}

	}

}
