package src;

import java.util.Scanner;

public class Ejercicio3 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int alt = 10;
		
		for(int i = 0; i < alt ;i++)
		{
			for (int j = 0; j < alt +1 ; j++) {
				System.out.print("*");
			}
			for (int j = 0; j < alt +1 ; j++) {
				System.out.print("!");
			}
			
			System.out.println("");
		}

	}

}
